# To learn more about .editorconfig see https://aka.ms/editorconfigdocs
root = true

# All files
[*]
indent_style = tab

# C# Files
[*.cs]
indent_style = tab
tab_width = 4
indent_size = 4
end_of_line = crlf

# VSSpell002: Ignore Word
dotnet_diagnostic.VSSpell002.severity = silent

# VSSpell001: Spell Check
dotnet_diagnostic.VSSpell001.severity = silent
